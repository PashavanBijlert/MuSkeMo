      \field{issn}{2517-4843}
      \field{journaltitle}{Integrative Organismal Biology}
      \field{langid}{english}
      \field{month}{9}
      \field{title}{Volumetric versus {{Element-scaling Mass Estimation}} and {{Its Application}} to {{Permo-Triassic Tetrapods}}}
      \field{urlday}{22}
      \field{urlmonth}{9}
      \field{urlyear}{2024}
      \field{year}{2024}
      \field{dateera}{ce}
      \field{urldateera}{ce}
      \field{pages}{obae034}
