    \entry{brasseyConvexhullMassEstimates2016}{article}{}{}
      \name{author}{5}{}{%
        {{hash=dfc9cd0bb3d49864209030514a041c96}{%
           family={Brassey},
           familyi={B\bibinitperiod},
           given={Charlotte\bibnamedelima A.},
           giveni={C\bibinitperiod\bibinitdelim A\bibinitperiod}}}%
        {{hash=8f6e775d8456971ba8263a39aa6f26aa}{%
           family={O’Mahoney},
           familyi={O\bibinitperiod},
