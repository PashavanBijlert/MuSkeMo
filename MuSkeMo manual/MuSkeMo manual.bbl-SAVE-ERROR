      \strng{namehash}{6f49ec228090d5e82f8b5702f8a9e352}
      \strng{fullhash}{6f49ec228090d5e82f8b5702f8a9e352}
      \strng{fullhashraw}{6f49ec228090d5e82f8b5702f8a9e352}
      \strng{bibnamehash}{6f49ec228090d5e82f8b5702f8a9e352}
      \strng{authorbibnamehash}{6f49ec228090d5e82f8b5702f8a9e352}
      \strng{authornamehash}{6f49ec228090d5e82f8b5702f8a9e352}
