      \strng{fullhashraw}{deb9fa8f7e5f3ccf8e2665df564ebbd8}
      \strng{bibnamehash}{556491dfeff30c7704c531a5d3fdb470}
      \strng{authorbibnamehash}{556491dfeff30c7704c531a5d3fdb470}
      \strng{authornamehash}{556491dfeff30c7704c531a5d3fdb470}
      \strng{authorfullhash}{deb9fa8f7e5f3ccf8e2665df564ebbd8}
      \strng{authorfullhashraw}{deb9fa8f7e5f3ccf8e2665df564ebbd8}
