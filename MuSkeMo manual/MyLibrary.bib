@article{wintrichPalaeontologicalEvidenceReveals2020,
  title = {Palaeontological Evidence Reveals Convergent Evolution of Intervertebral Joint Types in Amniotes},
  author = {Wintrich, Tanja and Scaal, Martin and Böhmer, Christine and Schellhorn, Rico and Kogan, Ilja and family=Reest, given=Aaron, prefix=van der, useprefix=true and Sander, P. Martin},
  date = {2020},
  journaltitle = {Scientific Reports},
  volume = {10},
  number = {1},
  eprint = {32839497},
