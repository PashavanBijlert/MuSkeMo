@article{xuPassiveStretchReduces2012,
  title = {Passive Stretch Reduces Calpain Activity through Nitric Oxide Pathway in Unloaded Soleus Muscles},
  author = {Xu, Peng Tao and Li, Quan and Sheng, Juan Juan and Chang, Hui and Song, Zhen and Yu, Zhi Bin},
