@article{renThreedimensionalLocomotorDynamics2008,
  title = {The Three-Dimensional Locomotor Dynamics of {{African}} ( {{{\mkbibemph{Loxodonta}}}}{\mkbibemph{ Africana}} ) and {{Asian}} ( {{{\mkbibemph{Elephas}}}}{\mkbibemph{ Maximus}} ) Elephants Reveal a Smooth Gait Transition at Moderate Speed},
  author = {Ren, Lei and Hutchinson, John R},
  date = {2008-02-06},
  journaltitle = {Journal of The Royal Society Interface},
  shortjournal = {J. R. Soc. Interface.},
  volume = {5},
  number = {19},
  pages = {195--211},
  issn = {1742-5689, 1742-5662},
  doi = {10.1098/rsif.2007.1095},
  url = {https://royalsocietypublishing.org/doi/10.1098/rsif.2007.1095},
  urldate = {2023-06-01},
  abstract = {We examined whether elephants shift to using bouncing (i.e. running) mechanics at any speed. To do this, we measured the three-dimensional centre of mass (CM) motions and torso rotations of African and Asian elephants using a novel multisensor method. Hundreds of continuous stride cycles were recorded in the field. African and Asian elephants moved very similarly. Near the mechanically and metabolically optimal speed (a Froude number (Fr) of 0.09), an inverted pendulum mechanism predominated. With increasing speed, the locomotor dynamics quickly but continuously became less like vaulting and more like bouncing. Our mechanical energy analysis of the CM suggests that at a surprisingly slow speed (approx. 2.2{$\mkern1mu$}m{$\mkern1mu$}s               −1               , Fr 0.25), the hindlimbs exhibited bouncing, not vaulting, mechanics during weight support. We infer that a gait transition happens at this relatively slow speed: elephants begin using their compliant hindlimbs like pogo sticks to some extent to drive the body, bouncing over their relatively stiff, vaulting forelimbs. Hence, they are not as rigid limbed as typically characterized for graviportal animals, and use regular walking as well as at least one form of running gait.},
  langid = {english},
  file = {C:\Users\3827658\Zotero\storage\I49NLA9T\Ren and Hutchinson - 2008 - The three-dimensional locomotor dynamics of Africa.pdf}
}

@article{rhodinTimingVerticalHead2022,
  title = {Timing of {{Vertical Head}}, {{Withers}} and {{Pelvis Movements Relative}} to the {{Footfalls}} in {{Different Equine Gaits}} and {{Breeds}}},
  author = {Rhodin, Marie and Smit, Ineke H. and Persson-Sjodin, Emma and Pfau, Thilo and Gunnarsson, Vikingur and Björnsdóttir, Sigridur and Zetterberg, Ebba and Clayton, Hilary M. and Hobbs, Sarah J. and Serra Bragança, Filipe and Hernlund, Elin},
