@article{demuthThreedimensionalPolygonalMuscle2022,
  title = {Three-Dimensional Polygonal Muscle Modelling and Line of Action Estimation in Living and Extinct Taxa},
  author = {Demuth, Oliver E. and Wiseman, Ashleigh L. A. and Van Beesel, Julia and Mallison, Heinrich and Hutchinson, John R.},
  date = {2022-03-01},
