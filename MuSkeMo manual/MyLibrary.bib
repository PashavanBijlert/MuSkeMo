@article{zhangAnatomicalStudyOstrich2016,
  title = {Anatomical Study of the Ostrich ({{Struthio}} Camelus) Foot Locomotor System},
  author = {Zhang, Rui and Wang, Haitao and Zeng, Guiyin and Zhou, Changhai and Pan, Runduo and Wang, Qiang and Li, Jianqiao},
  date = {2016-03-01},
