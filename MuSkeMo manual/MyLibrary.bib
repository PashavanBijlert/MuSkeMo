@article{andradaAdjustmentsGlobalHindlimb2013,
  title = {Adjustments of Global and Hindlimb Local Properties during the Terrestrial Locomotion of the Common Quail ( {{{\mkbibemph{Coturnix}}}}{\mkbibemph{ Coturnix}} )},
  author = {Andrada, Emanuel and Nyakatura, John A. and Bergmann, Florian and Blickhan, Reinhard},
