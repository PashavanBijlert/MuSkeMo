@article{whitePesAustralovenatorWintonensis2016,
  title = {The Pes of {{{\mkbibemph{Australovenator}}}}{\mkbibemph{ Wintonensis}} ({{Theropoda}}: {{Megaraptoridae}}): Analysis of the Pedal Range of Motion and Biological Restoration},
