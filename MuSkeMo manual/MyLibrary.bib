  shorttitle = {From Fibre to Function},
  author = {Charles, James and Kissane, Roger and Hoehfurtner, Tatjana and Bates, Karl T.},
  date = {2022-08},
  journaltitle = {Biological Reviews},
  shortjournal = {Biological Reviews},
