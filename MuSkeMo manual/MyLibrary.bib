  author = {Scholz, Melanie N and D'Août, Kristiaan and Bobbert, Maarten F and Aerts, Peter},
  date = {2006-09-07},
  journaltitle = {Proceedings of the Royal Society B: Biological Sciences},
  shortjournal = {Proc. R. Soc. B.},
  volume = {273},
  number = {1598},
