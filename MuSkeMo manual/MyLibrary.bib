@article{mitchellDifferentialDevelopmentalRates2023,
  title = {Differential Developmental Rates and Demographics in {{Red Kangaroo}} ({{{\mkbibemph{Osphranter}}}}{\mkbibemph{ Rufus}}) Populations Separated by the Dingo Barrier Fence},
  author = {Mitchell, D Rex and Cairns, Stuart C and Bradshaw, Corey J A and Saltré, Frédérik and Weisbecker, Vera},
  editor = {Greenville, Aaron},
  date = {2023-10-04},
  journaltitle = {Journal of Mammalogy},
  shortjournal = {J Mammal},
  volume = {104},
