@article{engScalingMuscleArchitecture2008,
  title = {Scaling of Muscle Architecture and Fiber Types in the Rat Hindlimb},
  author = {Eng, C. M. and Smallwood, L. H. and Rainiero, M. P. and Lahey, M. and Ward, S. R. and Lieber, R. L.},
  date = {2008},
  journaltitle = {Journal of Experimental Biology},
  volume = {211},
  number = {14},
  eprint = {18587128},
  eprinttype = {pubmed},
  pages = {2336--2345},
  issn = {0022-0949},
  doi = {10.1242/jeb.017640},
  url = {http://jeb.biologists.org/cgi/doi/10.1242/jeb.017640},
  abstract = {The functional capacity of a muscle is determined by its architecture\textbackslash nand metabolic properties. Although extensive analyses of muscle architecture\textbackslash nand fiber type have been completed in a large number of muscles in\textbackslash nnumerous species, there have been few studies that have looked at\textbackslash nthe interrelationship of these functional parameters among muscles\textbackslash nof a single species. Nor have the architectural properties of individual\textbackslash nmuscles been compared across species to understand scaling. This\textbackslash nstudy examined muscle architecture and fiber type in the rat (Rattus\textbackslash nnorvegicus) hindlimb to examine each muscle's functional specialization.\textbackslash nDiscriminant analysis demonstrated that architectural properties\textbackslash nare a greater predictor of muscle function (as defined by primary\textbackslash njoint action and anti-gravity or non anti-gravity role) than fiber\textbackslash ntype. Architectural properties were not strictly aligned with fiber\textbackslash ntype, but when muscles were grouped according to anti-gravity versus\textbackslash nnon-anti-gravity function there was evidence of functional specialization.\textbackslash nSpecifically, anti-gravity muscles had a larger percentage of slow\textbackslash nfiber type and increased muscle physiological cross-sectional area.\textbackslash nIncongruities between a muscle's architecture and fiber type may\textbackslash nreflect the variability of functional requirements on single muscles,\textbackslash nespecially those that cross multiple joints. Additionally, discriminant\textbackslash nanalysis and scaling of architectural variables in the hindlimb across\textbackslash nseveral mammalian species was used to explore whether any functional\textbackslash npatterns could be elucidated within single muscles or across muscle\textbackslash ngroups. Several muscles deviated from previously described muscle\textbackslash narchitecture scaling rules and there was large variability within\textbackslash nfunctional groups in how muscles should be scaled with body size.\textbackslash nThis implies that functional demands placed on muscles across species\textbackslash nshould be examined on the single muscle level.},
  isbn = {0022-0949},
  keywords = {muscle architecture,muscle fiber type,muscle mechanics,scaling},
  file = {C:\Users\3827658\Zotero\storage\EICTLFFB\Eng et al. 2008.pdf}
}

