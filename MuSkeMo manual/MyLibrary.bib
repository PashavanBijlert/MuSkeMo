@article{bhargavaPhenomenologicalModelEstimating2004a,
  title = {A Phenomenological Model for Estimating Metabolic Energy Consumption in Muscle Contraction},
  author = {Bhargava, Lindsay J. and Pandy, Marcus G. and Anderson, Frank C.},
