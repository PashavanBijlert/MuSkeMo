@article{picassoOntogeneticScalingHindlimb2015,
  title = {Ontogenetic {{Scaling}} of the {{Hindlimb Muscles}} of the {{Greater Rhea}} ({{{\mkbibemph{{\textsc{R}}}}}}{\mkbibemph{hea Americana}})},
  shorttitle = {Ontogenetic {{Scaling}} of the {{Hindlimb Muscles}} of the {{Greater Rhea}} ( {\mkbibemph{ }}},
