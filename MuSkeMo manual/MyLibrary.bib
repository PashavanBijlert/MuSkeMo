@article{snivelyLowerRotationalInertia2019,
  title = {Lower Rotational Inertia and Larger Leg Muscles Indicate More Rapid Turns in Tyrannosaurids than in Other Large Theropods},
  author = {Snively, Eric and O’Brien, Haley and Henderson, Donald M. and Mallison, Heinrich and Surring, Lara A. and Burns, Michael E. and Holtz, Thomas R. and Russell, Anthony P. and Witmer, Lawrence M. and Currie, Philip J. and Hartman, Scott A. and Cotton, John R.},
  date = {2019-02-21},
  journaltitle = {PeerJ},
  volume = {7},
