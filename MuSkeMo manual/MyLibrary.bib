@article{thorntonUnderstandingAustraliasUnique2022,
  title = {Understanding {{Australia}}’s Unique Hopping Species: A Comparative Review of the Musculoskeletal System and Locomotor Biomechanics in {{Macropodoidea}}},
  shorttitle = {Understanding {{Australia}}’s Unique Hopping Species},
  author = {Thornton, Lauren H. and Dick, Taylor J. M. and Bennett, Mike B. and Clemente, Christofer J.},
  editor = {Cooper, Paul},
  date = {2022-05-13},
  journaltitle = {Australian Journal of Zoology},
  shortjournal = {Aust. J. Zool.},
  volume = {69},
