@article{dejaegerIntermittentStretchTraining2015,
  title = {Intermittent Stretch Training of Rabbit Plantarflexor Muscles Increases Soleus Mass and Serial Sarcomere Number},
  author = {De Jaeger, Dominique and Joumaa, Venus and Herzog, Walter},
