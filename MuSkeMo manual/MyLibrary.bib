@article{lautenschlagerMultibodyDynamicsAnalysis2020,
  title = {Multibody Dynamics Analysis ( {{{\textsc{MDA}}}} ) as a Numerical Modelling Tool to Reconstruct the Function and Palaeobiology of Extinct Organisms},
  shorttitle = {Multibody Dynamics Analysis (},
  author = {Lautenschlager, Stephan},
