@article{athertonMechanosensitiveComponentsIntegrin2016,
  title = {Mechanosensitive Components of Integrin Adhesions: {{Role}} of Vinculin},
  author = {Atherton, Paul and Stutchbury, Ben and Jethwa, Devina and Ballestrem, Christoph},
  date = {2016},
  journaltitle = {Experimental Cell Research},
  volume = {343},
  number = {1},
  eprint = {26607713},
  eprinttype = {pubmed},
  pages = {21--27},
