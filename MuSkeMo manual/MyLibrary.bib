}

@article{millerTranstibialLimbLoss2021,
  title = {Transtibial Limb Loss Does Not Increase Metabolic Cost in Three-Dimensional Computer Simulations of Human Walking},
  author = {Miller, Ross H. and Russell Esposito, Elizabeth},
  date = {2021-08-04},
