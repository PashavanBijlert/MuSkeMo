@article{schaefferMorphologicalDisparityTheropod2020,
  title = {Morphological Disparity in Theropod Jaws: Comparing Discrete Characters and Geometric Morphometrics},
  shorttitle = {Morphological Disparity in Theropod Jaws},
  author = {Schaeffer, Joep and Benton, Michael J. and Rayfield, Emily J. and Stubbs, Thomas L.},
