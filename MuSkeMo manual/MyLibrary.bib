@inproceedings{shermanWhatMomentArm2013,
  title = {What Is a {{Moment Arm}}? {{Calculating Muscle Effectiveness}} in {{Biomechanical Models Using Generalized Coordinates}}},
  shorttitle = {What Is a {{Moment Arm}}?},
  booktitle = {Volume {{7B}}: 9th {{International Conference}} on {{Multibody Systems}}, {{Nonlinear Dynamics}}, and {{Control}}},
