@article{bishopUsingStepWidth2017,
  title = {Using Step Width to Compare Locomotor Biomechanics between Extinct, Non-Avian Theropod Dinosaurs and Modern Obligate Bipeds},
  author = {Bishop, P. J. and Clemente, C. J. and Weems, R. E. and Graham, D. F. and Lamas, L. P. and Hutchinson, J. R. and Rubenson, J. and Wilson, R. S. and Hocknull, S. A. and Barrett, R. S. and Lloyd, D. G.},
