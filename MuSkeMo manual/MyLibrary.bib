@article{renneTimeScalesCritical2013,
  title = {Time {{Scales}} of {{Critical Events Around}} the {{Cretaceous-Paleogene Boundary}}},
  author = {Renne, P. R. and Deino, A. L. and Hilgen, F. J. and Kuiper, K. F. and Mark, D. F. and Mitchell, W. S. and Morgan, L. E. and Mundil, R. and Smit, J.},
