@article{nuddsEvidenceEnergySavings2011,
  title = {Evidence for Energy Savings from Aerial Running in the {{Svalbard}} Rock Ptarmigan ( {{{\mkbibemph{Lagopus}}}}{\mkbibemph{ Muta Hyperborea}} )},
  author = {Nudds, R. L. and Folkow, L. P. and Lees, J. J. and Tickle, P. G. and Stokkan, K.-A. and Codd, J. R.},
  date = {2011-09-07},
  journaltitle = {Proceedings of the Royal Society B: Biological Sciences},
  shortjournal = {Proc. R. Soc. B.},
  volume = {278},
  number = {1718},
  pages = {2654--2661},
  issn = {0962-8452, 1471-2954},
  doi = {10.1098/rspb.2010.2742},
  url = {https://royalsocietypublishing.org/doi/10.1098/rspb.2010.2742},
  urldate = {2024-01-10},
  abstract = {Svalbard rock ptarmigans were walked and run upon a treadmill and their energy expenditure measured using respirometry. The ptarmigan used three different gaits: a walking gait at slow speeds (less than or equal to 0.75 m s               −1               ), grounded running at intermediate speeds (0.75 m s               −1               {$<$}               U               {$<$} 1.67 m s               −1               ) and aerial running at high speeds (greater than or equal to 1.67 m s               −1               ). Changes of gait were associated with reductions in the gross cost of transport (COT; J kg               −1               m               −1               ), providing the first evidence for energy savings with gait change in a small crouched-postured vertebrate. In addition, for the first time (excluding humans) a decrease in absolute metabolic energy expenditure (rate of O               2               consumption) in aerial running when compared with grounded running was identified. The COT versus               U               curve varies between species and the COT was cheaper during aerial running than grounded running, posing the question of why grounded running should be used at all. Existing explanations (e.g. stability during running over rocky terrain) amount to               just so               stories with no current evidence to support them. It may be that grounded running is just an artefact of treadmill studies. Research investigating the speeds used by animals in the field is sorely needed.},
  langid = {english},
  file = {C:\Users\3827658\Zotero\storage\BW4J4PZX\Nudds et al. - 2011 - Evidence for energy savings from aerial running in.pdf}
}

@article{nunamakerFatigueFracturesThoroughbred1990,
  title = {Fatigue Fractures in Thoroughbred Racehorses: {{Relationships}} with Age, Peak Bone Strain, and Training},
  shorttitle = {Fatigue Fractures in Thoroughbred Racehorses},
  author = {Nunamaker, D. M. and Butterweck, D. M. and Provost, M. T.},
  date = {1990-07},
  journaltitle = {Journal of Orthopaedic Research},
  shortjournal = {Journal Orthopaedic Research},
