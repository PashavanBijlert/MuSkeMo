@article{lernerModulatingTibiofemoralContact2015,
  title = {Modulating Tibiofemoral Contact Force in the Sheep Hind Limb via Treadmill Walking: {{Predictions}} from an {{OpenSim}} Musculoskeletal Model},
  shorttitle = {Modulating Tibiofemoral Contact Force in the Sheep Hind Limb via Treadmill Walking},
  author = {Lerner, Zachary F. and Gadomski, Benjamin C. and Ipson, Allison K. and Haussler, Kevin K. and Puttlitz, Christian M. and Browning, Raymond C.},
  date = {2015-08},
  journaltitle = {Journal of Orthopaedic Research},
  shortjournal = {J. Orthop. Res.},
