@article{goldspinkEffectDenervationAdaptation1974,
  title = {Effect of Denervation on the Adaptation of Sarcomere Number and Muscle Extensibility to the Functional Length of the Muscle.},
  author = {Goldspink, G and Tabary, C and Tabary, J C and Tardieu, C and Tardieu, G},
  date = {1974},
  journaltitle = {The Journal of physiology},
