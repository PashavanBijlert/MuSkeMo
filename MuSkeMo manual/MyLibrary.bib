@article{vanderzwaardMuscleMorphologyVastus2018,
  title = {Muscle Morphology of the Vastus Lateralis Is Strongly Related to Ergometer Performance, Sprint Capacity and Endurance Capacity in {{Olympic}} Rowers},
  author = {family=Zwaard, given=Stephan, prefix=van der, useprefix=true and Weide, Guido and Levels, Koen and Eikelboom, Michelle R. I. and Noordhof, Dionne A. and Hofmijster, Mathijs J. and family=Laarse, given=Willem J., prefix=van der, useprefix=true and family=Koning, given=Jos J., prefix=de, useprefix=true and family=Ruiter, given=Cornelis J., prefix=de, useprefix=true and Jaspers, Richard T.},
  date = {2018-09-17},
  journaltitle = {Journal of Sports Sciences},
