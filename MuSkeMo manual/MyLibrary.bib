@article{songDeepReinforcementLearning2021,
  title = {Deep Reinforcement Learning for Modeling Human Locomotion Control in Neuromechanical Simulation},
  author = {Song, Seungmoon and Kidziński, Łukasz and Peng, Xue Bin and Ong, Carmichael and Hicks, Jennifer and Levine, Sergey and Atkeson, Christopher G. and Delp, Scott L.},
  date = {2021-12},
