@article{alexanderFastLocomotionAfrican1977,
  title = {Fast Locomotion of Some {{African}} Ungulates},
  author = {family=Alexander, given=R. McN., given-i=R{{McN}} and Langman, V. A. and Jayes, A. S.},
  date = {1977-11},
  journaltitle = {Journal of Zoology},
